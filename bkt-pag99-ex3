#include <iostream>
#include <cstring>
#include <stdlib.h>
#include <fstream>
#pragma warning(disable:4996)


using namespace std;

int n;
int x[99];
int w = 0;

int suma(int k) {
    if (k == 1)
        return x[k];
    return x[k] + suma(k - 1);
}

int valid(int k) {
    if (suma(k) > n)
        return 0;
    if (k >= 2 && x[k - 1] >= x[k])
        return 0;
    return 1;
}

int sol(int k) {
    return suma(k) == n;
}

void afis(int k) {
    for (int i = 1; i < k; i++) {
        cout << x[i] << " + ";
    }
    cout << x[k];
    cout << "\n";
    w++;
}

void back(int k) {
    x[k] = 0;
    while (x[k] < n - 1) {
        x[k]++;
        if (valid(k))
            if (sol(k))
                afis(k);
            else
                back(k + 1);
    }
}

int main()
{
    cout << "n= ";
    cin >> n;
    back(1);
    cout << w;
    return 0;
}
