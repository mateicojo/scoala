int sol(int k) {
	if (k == n) {
		int pd = 0;
		int pi = 0;
		for (int i = 1; i <= n; i++) {
			if (pd < pi)
				return 0;
			if (x[i] == 1)
				pd++;
			else
				pi++;
		}
		return pd == pi;
	}
	return 0;
}

void afis(int k) {
	for (int i = 1; i <= k; i++)
		switch (x[i]) {
		case 1:
			cout << "(";
			break;
		case 2:
			cout << ")";
			break;
		}
	cout << "\n";
	w++;
}

void back(int k) {
	x[k] = 0;
	while (x[k] < 2 && k <= n) {
		x[k]++;
		if (valid(k))
			if (sol(k))
				afis(k);
			else
				back(k + 1);
	}
}

int main()
{
	cout << "n=";
	cin >> n;
	back(1);
	cout << "numarul de solutii " << w;
	return 0;
}
